{
  "name": "portfolio",
  "version": "0.1.0",
  "private": true,
  "homepage": "https://kristof-nyr.github.io/",
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^6.1.1",
    "@fortawesome/free-brands-svg-icons": "^6.1.1",
    "@fortawesome/react-fontawesome": "^0.1.18",
    "@material-ui/core": "^4.12.4",
    "@testing-library/jest-dom": "^5.16.2",
    "@testing-library/react": "^12.1.2",
    "@testing-library/user-event": "^13.5.0",
    "babel-plugin-macros": "^3.1.0",
    "clean-css": "^5.2.4",
    "font-awesome": "^4.7.0",
    "framer-motion": "^6.5.1",
    "node-sass": "^7.0.1",
    "popmotion": "^11.0.3",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-icons": "^4.4.0",
    "react-scripts": "^5.0.1",
    "web-vitals": "^2.1.4"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "compile-styles": "node-sass -w --output-style expanded --source-map true --source-map-contents true --precision 6 src/styles/ -o dist/styles",
    "sass-dev": "sass --watch --update --style=expanded src/styles/:src/output",
    "sass-prod": "sass --no-source-map --style=compressed src/styles/:src/output",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "gh-pages": "^4.0.0",
    "sass": "^1.49.7"
  }
}
